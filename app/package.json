{
  "name": "portefolio_app",
  "version": "1.0.0",
  "description": "frontend for the portef√≥lio blog",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=production node index.js",
    "dev": "concurrently \"nodemon -L ./dist/index.js\" \"npm run watch:server:dev\" \"npm run watch:client:dev\"",
    "watch:client:dev": "webpack --config webpack.client.config.js --env dev --watch",
    "watch:server:dev": "webpack --config webpack.server.config.js --env dev --watch",
    "build:client:dev": "webpack --config webpack.client.config.js --env dev",
    "build:server:dev": "webpack --config webpack.server.config.js --env dev",
    "watch:client:prod": "webpack --config webpack.client.config.js --env prod --watch",
    "watch:server:prod": "webpack --config webpack.server.config.js --env prod --watch",
    "build:client:prod": "webpack --config webpack.client.config.js --env prod",
    "build:server:prod": "webpack --config webpack.server.config.js --env prod"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/malbernaz/portefolio.git"
  },
  "keywords": [
    "react",
    "redux",
    "universal",
    "docker"
  ],
  "author": "Miguel Albernaz <albernazmiguel@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/malbernaz/portefolio/issues"
  },
  "homepage": "https://github.com/malbernaz/portefolio#readme",
  "dependencies": {
    "codemirror": "^5.17.0",
    "compression": "^1.6.2",
    "eslint": "^2.13.1",
    "eslint-plugin-jsx-a11y": "^1.5.5",
    "express": "^4.14.0",
    "highlight.js": "^9.5.0",
    "http-proxy": "^1.14.0",
    "lru-memoize": "^1.0.1",
    "marked": "^0.3.5",
    "meta-marked": "^0.4.1",
    "moment": "^2.14.1",
    "morgan": "^1.7.0",
    "react": "^15.2.1",
    "react-codemirror": "^0.2.6",
    "react-dom": "^15.2.1",
    "react-helmet": "^3.1.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.6.0",
    "react-router-redux": "^4.0.5",
    "react-router-scroll": "^0.2.1",
    "redux": "^3.5.2",
    "redux-form": "^5.3.1",
    "reselect": "^2.5.3",
    "serialize-javascript": "^1.3.0",
