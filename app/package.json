{
  "name": "portefolio_app",
  "version": "1.0.0",
  "description": "frontend for the portef√≥lio blog",
  "main": "index.js",
  "scripts": {
    "build": "yarn run clean && yarn build:client && yarn build:server",
    "build:client": "webpack --config webpack.client.config.js --env prod",
    "build:server": "webpack --config webpack.server.config.js --env prod",
    "watch": "webpack --config webpack.server.config.js --env dev --watch",
    "clean": "rm -fr dist && mkdir -p dist/public/img",
    "dev": "concurrently --raw --kill-others 'yarn nodemon' 'node wp-dev-server' 'yarn watch'",
    "imagemin": "imagemin src/static/img/* --out-dir=dist/public/img",
    "nodemon": "nodemon ./dist/index.js --ignore ./dist/public",
    "start": "yarn build && NODE_ENV=production node ./dist/index.js",
    "test": "NODE_ENV=test tape -r babel-register 'src/**/*.test.js' | faucet"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/malbernaz/portefolio.git"
  },
  "keywords": [
    "react",
    "redux",
    "universal",
    "docker"
  ],
  "author": "Miguel Albernaz <albernazmiguel@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/malbernaz/portefolio/issues"
  },
  "homepage": "https://github.com/malbernaz/portefolio#readme",
  "dependencies": {
    "compression": "^1.6.2",
    "cors": "^2.8.1",
    "express": "^4.14.0",
    "express-favicon": "^2.0.0",
    "http-proxy": "^1.15.1",
    "lodash": "4.17.2",
    "lru-memoize": "^1.0.1",
    "marked": "^0.3.6",
    "moment": "^2.16.0",
    "morgan": "^1.7.0",
    "react": "15.4.1",
    "react-addons-css-transition-group": "15.4.1",
    "react-dom": "15.4.1",
    "react-dropzone": "^3.7.3",
    "react-helmet": "^3.2.2",
    "react-redux": "4.4.6",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "react-router-scroll": "0.3.2",
    "redux": "^3.6.0",
    "redux-form": "^5.3.3",
    "reselect": "^2.5.4",
    "serialize-javascript": "^1.3.0",
    "serve-static": "^1.11.1",
    "spdy": "^3.4.4",
