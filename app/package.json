{
  "name": "portefolio_app",
  "version": "1.0.0",
  "description": "frontend for the portef√≥lio blog",
  "main": "index.js",
  "scripts": {
    "build:dev": "npm run clean && npm run build:client:dev && npm run build:server:dev",
    "build:prod": "npm run clean && npm run build:client:prod && npm run build:server:prod",
    "build:client:dev": "webpack --config webpack.client.config.js --env dev",
    "build:client:prod": "webpack --config webpack.client.config.js --env prod",
    "build:server:dev": "webpack --config webpack.server.config.js --env dev",
    "build:server:prod": "webpack --config webpack.server.config.js --env prod",
    "watch:client": "webpack --config webpack.client.config.js --env dev --watch",
    "watch:server": "webpack --config webpack.server.config.js --env dev --watch",
    "clean": "rm -fr dist && mkdir -p dist/public/img",
    "dev": "concurrently --raw --kill-others 'npm run nodemon' 'npm run watch:client' 'npm run watch:server'",
    "imagemin": "imagemin src/static/img/* --out-dir=dist/public/img",
    "nodemon": "nodemon -L ./dist/index.js --ignore ./dist/public",
    "start": "npm run build:prod && NODE_ENV=production node ./dist/index.js",
    "test": "NODE_ENV=test tape -r babel-register 'src/**/*.test.js' | faucet"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/malbernaz/portefolio.git"
  },
  "keywords": [
    "react",
    "redux",
    "universal",
    "docker"
  ],
  "author": "Miguel Albernaz <albernazmiguel@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/malbernaz/portefolio/issues"
  },
  "homepage": "https://github.com/malbernaz/portefolio#readme",
  "dependencies": {
    "compression": "^1.6.2",
    "draft-js": "^0.9.1",
    "express": "^4.14.0",
    "express-favicon": "^2.0.0",
    "http-proxy": "^1.15.1",
    "lodash": "^4.16.4",
    "lru-memoize": "^1.0.1",
    "marked": "^0.3.6",
    "moment": "^2.15.1",
    "morgan": "^1.7.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-dropzone": "^3.6.0",
    "react-helmet": "^3.1.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.1",
    "react-router-redux": "^4.0.6",
    "react-router-scroll": "^0.3.2",
    "redux": "^3.6.0",
    "redux-form": "^5.3.3",
    "reselect": "^2.5.4",
    "sanitize-html": "^1.13.0",
    "serialize-javascript": "^1.3.0",
    "serve-static": "^1.11.1",
    "spdy": "^3.4.3",
    "superagent": "^2.3.0",
    "sw-toolbox": "^3.3.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.0",
    "babel-core": "^6.16.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-lodash": "^3.2.9",
    "babel-plugin-transform-class-properties": "^6.16.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "concurrently": "^3.0.0",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.25.0",
    "cssnano": "^3.7.7",
    "eslint": "^3.7.0",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-import": "^2.0.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.4.0",
    "faucet": "0.0.1",
    "imagemin-cli": "^3.0.0",
    "isomorphic-style-loader": "^1.0.0",
    "json-loader": "^0.5.4",
    "lodash-webpack-plugin": "^0.10.2",
    "node-sass": "^3.10.1",
    "nodemon": "^1.11.0",
    "postcss-cssnext": "^2.8.0",
    "postcss-loader": "^0.13.0",
    "sass-loader": "^4.0.2",
    "sw-precache-webpack-plugin": "^0.5.1",
    "tape": "^4.6.2",
    "webpack": "^2.1.0-beta.25",
    "webpack-shell-plugin": "^0.4.3",
    "worker-loader": "^0.7.1"
  }
}
