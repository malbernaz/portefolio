{
  "name": "portefolio_app",
  "version": "1.0.0",
  "description": "frontend for the portef√≥lio blog",
  "main": "index.js",
  "scripts": {
    "build:dev": "npm run prebuild && npm run build:client:dev && npm run build:server:dev",
    "build:prod": "npm run prebuild && npm run build:client:prod && npm run build:server:prod",
    "build:client:dev": "webpack --config webpack.client.config.js --env dev --color",
    "build:client:prod": "webpack --config webpack.client.config.js --env prod --color",
    "build:server:dev": "webpack --config webpack.server.config.js --env dev --color",
    "build:server:prod": "webpack --config webpack.server.config.js --env prod --color",
    "watch:client:dev": "webpack --config webpack.client.config.js --env dev --watch --color",
    "watch:client:prod": "webpack --config webpack.client.config.js --env prod --watch --color",
    "watch:server:dev": "webpack --config webpack.server.config.js --env dev --watch --color",
    "watch:server:prod": "webpack --config webpack.server.config.js --env prod --watch --color",
    "clean": "rm -fr dist && mkdir -p dist/public/img",
    "dev": "concurrently --raw --kill-others 'npm run nm' 'npm run watch:client:dev' 'npm run watch:server:dev'",
    "imagemin": "imagemin src/static/img/* --out-dir=dist/public/img",
    "nm": "nodemon -L ./dist/index.js --ignore ./dist/public",
    "prebuild": "npm run clean && npm run static && npm run imagemin",
    "start": "NODE_ENV=production node index.js",
    "static": "cp src/static/manifest.json dist/public",
    "test": "NODE_ENV=test tape -r babel-register 'src/**/*.test.js' | faucet"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/malbernaz/portefolio.git"
  },
  "keywords": [
    "react",
    "redux",
    "universal",
    "docker"
  ],
  "author": "Miguel Albernaz <albernazmiguel@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/malbernaz/portefolio/issues"
  },
  "homepage": "https://github.com/malbernaz/portefolio#readme",
  "dependencies": {
    "compression": "^1.6.2",
    "draft-js": "^0.9.1",
    "express": "^4.14.0",
    "express-favicon": "^2.0.0",
    "highlight.js": "^9.7.0",
    "http-proxy": "^1.15.1",
    "lru-memoize": "^1.0.1",
    "marked": "^0.3.6",
    "moment": "^2.15.1",
    "morgan": "^1.7.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-dropzone": "^3.6.0",
    "react-helmet": "^3.1.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.1",
    "react-router-redux": "^4.0.6",
    "react-router-scroll": "^0.3.2",
    "redux": "^3.6.0",
    "redux-form": "^5.3.3",
    "reselect": "^2.5.4",
    "sanitize-html": "^1.13.0",
    "serialize-javascript": "^1.3.0",
    "serve-static": "^1.11.1",
    "spdy": "^3.4.3",
    "superagent": "^2.3.0",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.0",
    "babel-core": "^6.16.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-class-properties": "^6.16.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-export-extensions": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "concurrently": "^3.0.0",
    "css-loader": "^0.25.0",
    "cssnano": "^3.7.5",
    "eslint": "^3.7.0",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-import": "^2.0.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.3.0",
    "faucet": "0.0.1",
    "imagemin-cli": "^3.0.0",
    "isomorphic-style-loader": "^1.0.0",
    "json-loader": "^0.5.4",
    "node-sass": "^3.10.1",
    "nodemon": "^1.10.2",
    "offline-plugin": "^3.4.2",
    "postcss-cssnext": "^2.8.0",
    "postcss-loader": "^0.13.0",
    "sass-loader": "^4.0.2",
    "tape": "^4.6.2",
    "webpack": "^2.1.0-beta.25",
    "worker-loader": "^0.7.1"
  }
}
